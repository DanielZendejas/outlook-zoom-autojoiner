{
  "manifest_version": 2,
  "name": "Outlook Zoom autojoiner",
  "description": "To help you join your next Zoom meeting in a single click",
  "version": "1.0",

  "permissions": [
    "https://west.exch081.serverdata.net/*"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Outlook Zoom autojoiner",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
